{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": true,
  "properties": {
    "cluster": {
      "additionalProperties": false,
      "description": "list of Hazelcast clusters to create",
      "properties": {
        "name": {
          "default": "hz-first",
          "description": "Cluster name",
          "title": "name",
          "type": "string"
        },
        "size": {
          "default": 1,
          "description": "Number of members in the cluster",
          "minimum": 1,
          "title": "size",
          "type": "integer"
        }
      },
      "title": "cluster",
      "type": "object"
    },
    "licenseKey": {
      "default": "TrialLicense#10Nodes#.7Wh2kBN9XyTR6J4rwj18vvrMvFk37ckhw21VmVfKJ9-sYAEscbMOj20SDn7tzXkBqsurfXYpb3Ke0dMBrA0qDQ==",
      "description": "Hazelcast license key (leave empty if using Community)",
      "title": "licenseKey",
      "type": "string"
    },
    "repository": {
      "default": "docker.io/hazelcast/hazelcast",
      "description": "Repository for Hazelcast image",
      "title": "repository",
      "type": "string"
    },
    "version": {
      "default": "5.6.0",
      "description": "Hazelcast image version/tag",
      "title": "version",
      "type": "string"
    }
  },
  "type": "object"
}